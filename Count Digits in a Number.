#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N , count = 0;
    scanf("%d",&N);
    if (N<0){
        N=-N;
    }
    if (N==0){
        count = 1;
    }
    else{
        while(N>0){
            N =N/10;
            count ++;
        }
    }
    printf("%d",count);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
