#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char str[1000];
    int count = 0;
    int digit = 0; 
    int symbol = 0;
    scanf("%[^\n]",str);
    for(int i = 0; str[i] != '\0'; i++){
        char ch = str[i];
        if((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z')){
            count++;
            
            
        }
           else if (isdigit(ch)){
               digit++;
           }
           else{
               symbol++;
           }
           
    }
    printf("%d %d %d", count, digit , symbol);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
