#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void arr(int *a, int *b, int n){
    for(int i = 0; i<n; i++){
        int temp = *(a+i);
        *(a+i) = *(b+i);
        *(b+i) = temp;
       
        
    }
}

int main() {
    int n; 
    scanf("%d",&n);
    
    int a[100], b[100];
    for(int i = 0 ; i < n ; i++){
        scanf("%d", (a+i));
    }
    for(int i = 0 ; i < n ; i++){
        scanf("%d", (b+i));
    }
    arr(a,b,n);
    for(int i = 0 ; i < n ; i++){
        printf("%d ", *(a+i));
    }
    
        printf("\n");
    for(int i = 0 ; i < n ; i++){
        printf("%d ",*(b+i));
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
