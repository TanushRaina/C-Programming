#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, count = 0 , key ;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0; i<n ;i++){
        scanf("%d", &arr[i]);
        
    }
    scanf("%d",&key);
    
    for(int i =0 ; i<n ; i++){
        if(key == arr[i]){
            count++;
            
        }
    }
    printf("%d",count);
    return 0;
}
