#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n; 
    scanf("%d",&n);
    int arr[1000];
    for(int i = 0 ; i<n ; i++) scanf("%d",&arr[i]);
    int count = 0 ; 
    for(int i = 0 ; i<n ; i++){
        if(arr[i] == 0) count ++;
    }
    if(count > 0) printf("0: %d",count);
    for(int i = 0 ; i<n ; i++) {
        if(arr[i]!= 0){
            count = 1;
            for(int j = i+1 ; j <n ; j++) {
                if(arr[j] == arr[i]) {
                    count++;
                    arr[j] = 0;
                }
            }
            printf("%d: %d\n",arr[i],count);
        }
    }


    
    return 0;
}
