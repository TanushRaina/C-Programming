#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main() {
    char str[1001];
     char clean[1001];
    scanf("%[^\n]s",str);
    int flag = 1;
    int i = 0;
    int j = 0;
    while(str[i]!= '\0'){
      clean[j] = tolower(str[i]);
                   i++;
        j++;
                          } 
                         
                          

            
                          
        clean[j] = '\0';
        
       

   
    
    int len = 0;
     while(str[len]!= '\0'){
         len++;
     }
    
    
    for(int k = 0; k<len/2 ; k++){
        if(clean[k] != clean[len - 1 -k ]){
            flag = 0;
            break;
            
        }
    }
    if(flag == 1){
        printf("Palindrome");
        
    }
    else{
        printf("Not Palindrome");
    }
    return 0;
}
