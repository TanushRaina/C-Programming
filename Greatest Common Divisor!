#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,temp;
    scanf("%d %d", &a,&b);
    while(b!=0){
        temp = b;
        b = a%b;
        a = temp;
        
    }
    printf("%d",a);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
