#include <stdio.h>
#include <string.h>
#include <math.h>


int main() {
    int a, b; 
    scanf("%d %d",&a,&b);
    int *p1;
    int *p2;
    p1 = &a;
    p2 = &b;
    int sum = *p1 + *p2;
    
    printf("%d",sum);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
