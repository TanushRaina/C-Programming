#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    int word = 0; 
    int count = 0;
    char str[2001];
    scanf("%[^\n]s",str);
    for(int i = 0; str[i] != '\0';i++){
        if(isalpha(str[i])){
            if(word == 0){
                word = 1;
                count++;
            }
            
        }
        else {
            word = 0; 
        }
    }
    printf("%d",count);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
