#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, ist, last , temp, digits = 0;
    scanf("%d",&n);
    temp = n;
    last = n%10;
    while (temp>=10){
        temp = temp/10;
    }
    ist = temp;
    temp = n;
    while(temp>0){
        temp = temp/10;
        digits ++ ;
    }
    int mid = n%(int)(pow(10,digits-1));
    mid = mid/10;
    int swap = last*(int)(pow(10,digits - 1)) + mid*10 + ist;
    printf("%d",swap);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
