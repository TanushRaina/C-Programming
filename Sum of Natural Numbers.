#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,sum;
    scanf("%d",&N);
    for(int i = 1 ; i<=N; i++){
        sum+=i;
    }
    printf("%d\n",sum);
        
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
