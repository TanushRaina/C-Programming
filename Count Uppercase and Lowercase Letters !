#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[1001];
    int size = 0; 
    int count =0;
    scanf("%[^\n]",str);
    for(int i = 0; str[i] != '\0'; i++){
        char ch = str[i];
        if(ch >= 'A' && ch <= 'Z') {
            count++;
        
        }
        else if( ch >= 'a' && ch <= 'z'){
              size++;
        
        }
           }
        printf("%d %d",count , size);
              
    
    
    
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
           }
