#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int arrsum(int arr[], int i, int n){
    if(i == n) return 0;
    return arr[i] + arrsum(arr, i+1, n);
}

int main() {
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i = 0 ; i<n; i++){
        scanf("%d",&a[i]);
    }
    int sum = arrsum(a,0,n);
    printf("%d\n",sum);


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
