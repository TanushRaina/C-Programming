#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct std{
    char name[50];
    int age;
    char grade;
    
    
};


int main() {
    int n;
    scanf("%d",&n);
    struct std a[n];
    for(int i = 0; i<n; i++){
    scanf(" %[^\n]%*c",a[i].name);
    scanf("%d",&a[i].age);
    scanf(" %c",&a[i].grade);
    }
    for(int i = 0; i<n; i++){
        printf("Student %d:\n",i+1);
        printf("Name: %s\n",a[i].name);
        printf("Age: %d\n",a[i].age);
        printf("Grade: %c\n",a[i].grade);
    }
    

     
    return 0;
}
