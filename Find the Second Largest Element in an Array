#include <stdio.h>
#include <string.h>
#include <math.h>
#include <limits.h>

int main() {
    int n, found =0; 
    scanf("%d",&n);
    int arr[n];
    int max = INT_MIN;
    int smax = INT_MIN;
    for(int i = 0 ; i<n; i++){
        scanf("%d",&arr[i]);
    }
    
    
        for(int i = 0 ; i<n; i++){
            if(max<arr[i]){
                max =arr[i];
            }
        }
    for(int i = 0 ; i<n; i++){
            if(arr[i] != max && smax<arr[i]){
               smax =arr[i];
                found = 1 ;
            }
        }
    if(found == 0 ){
        printf("No second largest element");
    }
    else{
    printf("%d",smax);
        }
    
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
