#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n; 
    scanf("%d",&n);
    int arr[n];
    for(int i= 0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    int value;
    scanf("%d",&value);
    int position;
    scanf("%d",&position);
    int index = position - 1;
    
    for(int i =n; i>index; i--){
        arr[i] = arr[i-1];
    }
    arr[index] = value;
    n++;
    for(int i= 0; i<n; i++){
    
    printf("%d ",arr[i]);
}

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
