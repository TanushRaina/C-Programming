#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int rev(int  n , int x){
    if(n==0) return x ;
    return rev(n/10,x*10+(n%10));
    
}

int main() {
    int n; 
    scanf("%d",&n);
    
   int reverse =  rev(n,0);
    printf("%d",reverse);
    return 0;
    
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
