#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[1001];
    int size = 0; 
    int count =0;
    scanf("%[^\n]",str);
    for(int i = 0; str[i] != '\0'; i++){
        char ch = str[i];
        if((ch >= 'A' && ch <= 'Z') ||( ch >= 'a' && ch <= 'z')){
            count++;
        
        if(ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U' || ch == 'a' || ch == 'e' || ch =='i' || ch == 'o' || ch == 'u'){
              size++;
            
        }
              
    }
    }
    int cons = count - size;
    printf("%d %d",size,cons);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
